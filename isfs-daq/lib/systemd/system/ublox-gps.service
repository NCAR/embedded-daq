[Unit]
Description=Configure Ublox GPS via I2C
ConditionFileIsExecutable=/opt/nidas/bin/ublox
Requires=tee-gps.service
Before=tee-gps.service

[Service]
Type=oneshot
EnvironmentFile=/etc/default/tee-i2c-gps
ExecStart=/opt/nidas/bin/ublox $GPS_DEVICE $GPS_TEE_OPTS -d VTG -d GSV -d GSA -d GLL

[Install]
WantedBy=tee-gps.service
