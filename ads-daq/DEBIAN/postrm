#!/bin/sh
# postrm script for ads-daq
#
# see: dh_installdeb(1)

set -e

# targets: purge|remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear

case "$1" in
    purge)
    ;;

    remove)
    ;;

    upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)

    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installinit/11.1.6ubuntu1
if [ "$1" = "purge" ] ; then
        if `which ucf >/dev/null`
        then
            ucf --purge /etc/modules-load.d/ads.conf
            if [ -x "$(which ucfr)" ]; then
                ucfr --purge ads-daq /etc/modules-load.d/ads.conf
            fi
        fi
fi


